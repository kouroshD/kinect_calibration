<launch>
	<arg name="marker_size" default="6.2" /> 
	<arg name="max_new_marker_error" default="0.08" />
	<arg name="max_track_error" default="0.00" />

	<arg name="cam_image_topic" default="/kinect2/qhd/image_color_rect" />
	<arg name="cam_info_topic" default="/kinect2/qhd/camera_info" />		
	<arg name="output_frame" default="/kinect2_rgb_optical_frame" />
	<arg name="bundle_files" default="$(find ar_track_alvar)/bundles/emaroLab_table_8_9_10.xml" />
	<arg name="median_filt_size" default="3" />

	<node name="ar_track_alvar" pkg="ar_track_alvar" type="findMarkerBundlesNoKinect" respawn="false" output="screen" args="$(arg marker_size) $(arg max_new_marker_error) $(arg max_track_error) $(arg cam_image_topic) $(arg cam_info_topic) $(arg output_frame) $(arg bundle_files)" />
</launch>

